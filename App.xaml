                    <AcrylicBrush x:Key="AcrylicText" BackgroundSource="HostBackdrop" TintColor="Black" FallbackColor="DarkGray" TintOpacity="0.6"></AcrylicBrush>
                    <AcrylicBrush x:Key="NavigationViewTopPaneBackground" BackgroundSource="Backdrop" TintColor="#FFE3E3E3" TintOpacity="0.6" FallbackColor="#FFE3E3E3"></AcrylicBrush>
                    <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="#FFFEFEFF"></SolidColorBrush>
